{
  "name": "@gnx-utilities/core",
  "version": "1.0.77",
  "description": "GNX is a collect of utilities that are used across all of our projects. It is a collection of utilities that we have found useful in our projects and we hope you will find them useful in yours. We have tried to make them as generic as possible so that they can be used in any project.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
    "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./lib/index.js": "./lib/index.js",
    "./lib/index.d.ts": "./lib/index.d.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "files": [
    "lib/**/*.js",
    "lib/**/*.cjs",
    "lib/**/*.d.ts",
    "lib/**/*.d.cts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ImRLopezAG/generic-service.git"
  },
  "homepage": "https://gnx-udocs.vercel.app",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "sequelize",
    "mongoose",
    "typegoose",
    "generic"
  ],
  "author": "ImRLopezAg",
  "license": "MIT",
  "scripts": {
    "build": "turbo build --log-order=grouped",
    "build:pkg": "pnpm build && tsup",
    "dev": "turbo dev",
    "test": "turbo test --log-order=grouped",
    "build:test": "pnpm build:pkg && pnpm test",
    "build:models": "turbo build --filter @gnx-utilities/models",
    "build:services": "turbo build --filter @gnx-utilities/services",
    "build:decorators": "turbo build --filter @gnx-utilities/decorators",
    "build:validators": "turbo build --filter @gnx-utilities/validators",
    "build:controllers": "turbo build --filter @gnx-utilities/controllers",
    "build:cli": "turbo build --filter @gnx-utilities/cli",
    "test:services": "turbo test --filter @gnx-utilities/services --log-order=grouped",
    "test:decorators": "turbo test --filter @gnx-utilities/decorators --log-order=grouped",
    "test:validators": "turbo test --filter @gnx-utilities/validators --log-order=grouped",
    "test:controllers": "turbo test --filter @gnx-utilities/controllers --log-order=grouped",
    "pub:services": "turbo publish --filter @gnx-utilities/services",
    "pub:models": "turbo publish --filter @gnx-utilities/models",
    "pub:validators": "turbo publish --filter @gnx-utilities/validators",
    "pub:decorators": "turbo publish --filter @gnx-utilities/decorators",
    "pub:controllers": "turbo publish --filter @gnx-utilities/controllers",
    "pub:cli": "turbo publish --filter @gnx-utilities/cli"
  },
  "dependencies": {
    "@gnx-utilities/decorators": "*",
    "@gnx-utilities/models": "*",
    "@gnx-utilities/services": "*",
    "@gnx-utilities/validators": "*",
    "@gnx-utilities/cli": "*",
    "@gnx-utilities/controllers": "*",
    "mongoose": "^8.0.2",
    "sequelize": "^6.35.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@repo/eslint-config": "workspace:*",
    "@repo/ts-config": "workspace:*",
    "@swc/core": "^1.3.100",
    "turbo": "^1.11.2",
    "tsup": "^8.0.1"
  },
  "packageManager": "pnpm@8.10.5"
}
