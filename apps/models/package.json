  "author": "ImRLopezAg",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "tsup": "^8.0.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@typegoose/typegoose": "^12.0.0",
    "mongoose": "^8.0.2",
    "sequelize": "^6.35.1"
  },
  "repository": {
