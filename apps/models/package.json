{
  "name": "@gnx-utilities/models",
  "version": "0.1.1",
  "description": "Generic services is a library that allows you to create services with a generic repository",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=16.20.1"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "scripts": {
    "start": "pnpm build && node ./lib/index.js",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "tsup && node --test"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "express",
    "sequelize",
    "mongoose",
    "typegoose",
    "generic"
  ],
  "author": "ImRLopezAg",
  "license": "MIT",
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/ts-config": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@typegoose/typegoose": "^12.0.0",
    "mongoose": "^8.0.2",
    "sequelize": "^6.35.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ImRLopezAG/generic-service.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
